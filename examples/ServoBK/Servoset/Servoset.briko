
servobk motor(PORT1);    //Declara el modulo de servo en el puerto 1

//Codigo principal que corre por siempre
code(){

  motor.set(0);           //Pone el servo a 0 grados
  delay(1000);          //Espera 1000 milisegundos
  motor.set(180);         //Pone el servo a 180 grados
  delay(1000);          //Espera 1000 milisegundos

}

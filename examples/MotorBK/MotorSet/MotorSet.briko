
motorbk motor(PORT1);   //Declara el modulo MotorBK en el puerto 1

//Codigo principal que corre por siempre
code(){

   motor.set(RIGHT,255);    //Prende el motor a la velocidad deseada girando en sentido de las manecillas del reloj
   delay(1000);             //Delay de 1000 milisegundos
   motor.set(LEFT);         //Cambia la direccion del motor
   delay(1000);             //Delay de 1000 milisegundo

}

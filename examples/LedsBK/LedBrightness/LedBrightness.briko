
ledsbk leds(PORT1);     //Declara el modulo de leds en el puerto 1

//Codigo principal que corre por siempre
code(){

leds.color(1,WHITE);      //Prende el primer led de color blanco
leds.brightness(255);     //Cambia la intensidad del led a 255
delay(1000);              //Espera 1000 milisegundos
leds.brightness(100);     //Cambia la intensidad del led a 100
delay(1000);              //Espera 1000 milisegundos

}


char Var;

//Codigo principal que corre por siempre
code(){

    Var = bk7read();  // Prende el led del BK7
    if(Var == 'a'){
    bk7led(ON);       // Prende el led del BK7
    delay(1000);      // Espera 1000 milisegundos
    bk7led(OFF);      // Apaga el led del BK7
    }
delay(10);            // Espera 10 milisegundos

}
